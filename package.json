  "author": "Guy Royse <guy@guyroyse.com> (http://guyroyse.com/)",
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/uuid": "^8.3.1",
    "jest": "^27.2.4",
    "nodemon": "^2.0.13",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.22.7",
    "typedoc-plugin-markdown": "^3.11.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "redis": "^4.0.0-rc.3",
    "ulid": "^2.3.0"
